{
  "name": "feathers-reactvm-boilerplate",
  "version": "0.0.1",
  "description": "Feathers & ReactVM Boilerplate.  Requires separate Feathers server.",
  "main": "index.html",
  "scripts": {
    "test": "npm run lint && testee src/test.html --browsers firefox --reporter Spec",
    "start": "concurrently \"npm run develop\" \"npm run feathers\" \"steal-tools live-reload\" ",
    "develop": "done-serve --develop --port 8080 --static",
    "feathers": "node ./server/src/index",
    "document": "documentjs",
    "build": "node build",
    "lint": "semistandard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feathersjs/feathers-reactvm-boilerplate.git"
  },
  "keywords": [
    "feathersjs",
    "admin-ui"
  ],
  "author": "Marshall Thompson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feathersjs/feathers-reactvm-boilerplate/issues"
  },
  "homepage": "https://github.com/feathersjs/feathers-reactvm-boilerplate#readme",
  "dependencies": {
    "can-compute": "^3.0.3",
    "can-connect": "^1.0.11",
    "can-define": "^1.0.4",
    "can-route": "^3.0.3",
    "can-route-pushstate": "^3.0.0",
    "canjs-feathers": "^1.0.1",
    "classnames": "^2.2.5",
    "feathers": "^2.0.2",
    "feathers-authentication": "^0.7.10",
    "feathers-authentication-popups": "^0.1.2",
    "feathers-hooks": "^1.5.7",
    "feathers-reactive": "^0.4.0",
    "feathers-socketio": "^1.4.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-view-models": "0.0.7",
    "rxjs": "^5.0.0-beta.12",
    "steal-socket.io": "^4.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "done-serve": "^0.3.0-pre.0",
    "mocha": "^3.0.2",
    "semistandard": "^9.0.0",
    "steal": "^1.0.0-rc.3",
    "steal-builtins": "^1.0.0",
    "steal-mocha": "0.0.3",
    "steal-tools": "^1.0.0-rc.7",
    "testee": "^0.3.0-pre.1"
  },
  "system": {
    "main": "app.js",
    "directories": {
      "lib": "src"
    },
    "transpiler": "babel",
    "babelOptions": {
      "blacklist": []
    },
    "configDependencies": [
      "live-reload"
    ],
    "ext": {
      "jsx": "steal-jsx"
    }
  }
}
